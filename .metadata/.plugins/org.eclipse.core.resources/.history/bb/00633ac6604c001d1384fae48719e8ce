package intro;

public class Main {
	public static void main(String[] args) {
		
		double dolarYesterday = 18.55;
		double dolarToday = 18.65;
		String situation = "";
		
		if(dolarToday>dolarYesterday) {//true
			situation = "up.svg";
			System.out.println(situation);
		}else if(dolarYesterday>dolarToday) {
			situation = "down.svg";
			System.out.println(situation);
		}else if(dolarToday == dolarYesterday) {
			situation = "equal.svg";
			System.out.println(situation);
		}else {
			System.out.println("Bir Şeyler Yanlış Gitti!");
		}
		
		
		//camelCase
		//PascalCase
		//Upper_Case
	}
}