package intro;

public class Main {
	public static void main(String[] args) {
		
		double dolarYesterday = 18.15;
		double dolarToday = 18.10;

		
		if (dolarToday>dolarYesterday) {
			System.out.println("Dolar dün : "+dolarYesterday);
			System.out.println("Dolar bugün : "+dolarToday);
			System.out.println("Dolar yükseldi");
		}
		else {
			System.out.println("Dolar dün : "+dolarYesterday);
			System.out.println("Dolar bugün : "+dolarToday);
			System.out.println("Dolar düştü");
		}
		
		String[] myArray = {};
		
		for (int i = 0; i < myArray.leght;i++) {
			
		}
	}
}
